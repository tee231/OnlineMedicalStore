@model OnlineMedicineStore.Models.RegisterViewModel

@{
    ViewData["Title"] = "Register";
    Layout = "~/Views/Shared/_LayoutTaiwo.cshtml";
}
<meta charset="UTF-8">
<meta name="viewport" content="width=device-width, initial-scale=1">
<!--===============================================================================================-->
<link rel="icon" type="image/png" href="/images/icons/favicon.ico" />
<!--===============================================================================================-->
<link rel="stylesheet" type="text/css" href="/vendor/bootstrap/css/bootstrap.min.css">
<!--===============================================================================================-->
<link rel="stylesheet" type="text/css" href="/fonts/font-awesome-4.7.0/css/font-awesome.min.css">
<!--===============================================================================================-->
<link rel="stylesheet" type="text/css" href="/fonts/iconic/css/material-design-iconic-font.min.css">
<!--===============================================================================================-->
<link rel="stylesheet" type="text/css" href="vendor/animate/animate.css">
<!--===============================================================================================-->
<link rel="stylesheet" type="text/css" href="/vendor/css-hamburgers/hamburgers.min.css">
<!--===============================================================================================-->
<link rel="stylesheet" type="text/css" href="/vendor/animsition/css/animsition.min.css">
<!--===============================================================================================-->
<link rel="stylesheet" type="text/css" href="/vendor/select2/select2.min.css">
<!--===============================================================================================-->
<link rel="stylesheet" type="text/css" href="/vendor/daterangepicker/daterangepicker.css">
<!--===============================================================================================-->
<link rel="stylesheet" type="text/css" href="/css/util.css">
<link rel="stylesheet" type="text/css" href="/css/main.css">
<!--===============================================================================================-->
@*</head>*@
<body>
    
    <div class="limiter">
        <div class="container-login100" style="background-image: url('~/images/bg-01.jpg');">
            <div class="wrap-login100">
                <form asp-route-returnUrl="@ViewData["ReturnUrl"]" method="post">
                    <span class="login100-form-logo">
                        <i class="zmdi zmdi-landscape"></i>
                    </span>

                    <span class="login100-form-title p-b-34 p-t-27">
                        REGISTER
                    </span>
                    <div asp-validation-summary="All" class="text-danger"></div>

                    <div class="wrap-input100">
                        <label asp-for="FirstNames"></label>
                        <input asp-for="FirstNames" class="input100" type="text" />
                        <span asp-validation-for="FirstNames" class="focus-input100" ></span>

                    </div>
                    <div class="wrap-input100">
                        <label asp-for="LastNames"></label>
                        <input asp-for="LastNames" class="input100" type="text" />
                        <span asp-validation-for="LastNames" class="focus-input100"></span>
                    </div>
                    <div class="wrap-input100">
                        <label asp-for="usernames"></label>
                        <input asp-for="usernames" class="input100" type="text" />
                        <span asp-validation-for="usernames" class="focus-input100"></span>
                    </div>
                    <div class="wrap-input100">
                        <label asp-for="HomeAddress"></label>
                        <input asp-for="HomeAddress" class="input100" type="text" />
                        <span asp-validation-for="HomeAddress" class="focus-input100" ></span>
                    </div>
                    <div class="wrap-input100">
                        <label asp-for="Email"></label>
                        <input asp-for="Email" class="input100" type="text" />
                        <span asp-validation-for="Email" class="focus-input100"></span>
                    </div>
                    <div class="wrap-input100">
                        <label asp-for="Password"></label>
                        <input asp-for="Password" class="input100"  />
                        <span asp-validation-for="Password" class="focus-input100"></span>
                    </div>
                    <div  class="wrap-input100">
                        <label asp-for="ConfirmPassword"></label>
                        <input asp-for="ConfirmPassword" class="input100" />
                        <span asp-validation-for="ConfirmPassword" class="focus-input100"></span>
                    </div>

                    <div class="wrap-input100">
                        <label asp-for="Dob"></label>
                        <input asp-for="Dob" class="input100"  />
                        <span asp-validation-for="Dob" class="focus-input100"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="PhoneNumber"></label>
                        <input asp-for="PhoneNumber" class="input100" type="text" />
                        <span asp-validation-for="PhoneNumber" class="focus-input100"></span>
                    </div>
                    <div class="container-login100-form-btn">
                        <button class="login100-form-btn">
                            Register
                        </button>
                    </div>

                </form>
            </div>
        </div>
    </div>
    @section Scripts {
        @await Html.PartialAsync("_ValidationScriptsPartial")
    }
    <div id="dropDownSelect1"></div>

    <!--===============================================================================================-->
    <script src="~/vendor/jquery/jquery-3.2.1.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/vendor/animsition/js/animsition.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/vendor/bootstrap/js/popper.js"></script>
    <script src="~/vendor/bootstrap/js/bootstrap.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/vendor/select2/select2.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/vendor/daterangepicker/moment.min.js"></script>
    <script src="~/vendor/daterangepicker/daterangepicker.js"></script>
    <!--===============================================================================================-->
    <script src="~/vendor/countdowntime/countdowntime.js"></script>
    <!--===============================================================================================-->
    <script src="~/js/main.js"></script>

</body>
